{
  "schemaVersion": 1,
  "id": "vowcloud",
  "version": "${version}",
  "name": "Vow Cloud",
  "description": "Provides voice acting to Wynncraft npcs which is streamed through a server",
  "authors": [
    "kmaxi"
  ],
  "contact": {
    "website": "https://voicesofwynn.com",
    "repo": "https://github.com/kmaxii/VowCloud",
    "issues": "https://github.com/kmaxii/VowCloud/issues"
  },
  "license": "GPL",
  "icon": "assets/vowcloud/icon.png",
  "environment": "client",
  "entrypoints": {
    "main": [
      "me.kmaxi.vowcloud.VowCloud"
    ],
    "modmenu": [
      "me.kmaxi.vowcloud.config.integration.ModMenuIntegration"
    ]
  },
  "mixins": [
    "vowcloud.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}",
    "fabric-api": ">=${fabric_version}"
  },
  "suggests": {
    "modmenu": "*",
    "cloth-config2": ">=${cloth_config_version}"
  }
}
